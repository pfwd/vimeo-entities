{
    "name": "pfwd/vimeo-entities",
    "description": "A set of PHP entities that represent the Vimeo API responses",
    "type": "library",
    "keywords": ["vimeo","entities", "model layer"],
    "homepage": "https://github.com/pfwd/vimeo-entities",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Fisher",
            "homepage": "http://peterfisher.me.uk",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "PFWD\\Vimeo\\": "src/Vimeo/"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.3",
        "codeception/codeception": "^2.5"
    },
    "config": {
        "bin-dir": "bin"
    }
